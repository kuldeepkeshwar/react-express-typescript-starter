{
  "name": "react-express-typescript-starter",
  "version": "1.0.0",
  "description": "starter project for react,express,typescript,jest,webpack,mysql",
  "scripts": {
    "clean": "rimraf dist",
    "clean-server": "rimraf dist/index.js ",
    "clean-client": "rimraf dist/static ",
    "build": "rimraf dist && npm run build-client && npm run build-server ",
    "build-client": "NODE_ENV=production webpack --config tools/build/webpack-client.config.js --colors",
    "build-server": "NODE_ENV=production webpack --config tools/build/webpack-server.config.js --colors",
    "build-client:dev": "NODE_ENV=development webpack-dev-server --config tools/build/webpack-client.config.js --colors --watch",
    "build-server:dev": "npm run build-server -- --watch",
    "start:dev": "pm2 start tools/server/ecosystem-dev.config.js",
    "start": "pm2 start tools/server/ecosystem.config.js",
    "lint": "npm run lint-client && npm run lint-server",
    "lint-client": "cd src/client && tslint --project tsconfig.json",
    "lint-server": "cd src/server && tslint --project tsconfig.json",
    "test": "npm run test-client && npm run test-server && npm run coverage-ratchet:client && npm run coverage-ratchet:server",
    "test-client": "cd src/client && jest --config jest-config.json --coverage --bail",
    "test-server": "cd src/server && jest --config jest-config.json --coverage --bail",
    "coverage-ratchet:client": "jest-coverage-ratchet --configPath ./src/client/jest-config.json --coverageSummaryPath coverage/client/coverage-summary.json",
    "coverage-ratchet:server": "jest-coverage-ratchet --configPath ./src/server/jest-config.json --coverageSummaryPath coverage/server/coverage-summary.json"
  },
  "keywords": [
    "react",
    "express",
    "typescript",
    "jest",
    "webpack",
    "hrm",
    "mysql"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuldeepkeshwar/react-express-typescript-starter"
  },
  "author": "Kuldeep Keshwar",
  "license": "MIT",
  "devDependencies": {
    "@types/history": "2.0.44",
    "@types/jest": "^21.1.2",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.9",
    "@types/react-router": "2.0.42",
    "@types/react-router-redux": "4.0.36",
    "@types/webpack-env": "^1.13.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.16.1",
    "jest": "^21.2.1",
    "jest-coverage-ratchet": "git+https://github.com/kuldeepkeshwar/jest-coverage-ratchet.git",
    "jquery": "^2.2.3",
    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^3.2.0",
    "source-map-support": "^0.4.18",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "bunyan-rotating-file-stream": "^1.6.3",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.13.4",
    "express-mysql-session": "^1.2.2",
    "express-session": "^1.15.6",
    "helmet": "^3.8.2",
    "morgan": "^1.8.2",
